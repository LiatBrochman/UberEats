      "bundleIdentifier": "com.joey123ms.uberEatsCourier"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.joey123ms.uberEatsCourier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7dfa86b5-8fd6-4353-831b-bb2b1db7f57f"
      }
    },
    "scheme": "ubereats"
  }
}
